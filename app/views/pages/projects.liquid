---
title: "Projects"
published: true
---

{% extends index %}

{% block 'header' %}

  <div class="header">
    <div class="jumbotron" id="section-development-project">
      <div class="container">
        {% assign url = 'applidget-logo/blue.png' %}
        {% include 'topmenu' %}
      </div>
      <div class="container">
        <div class="row">
          <div class="span5">
            <img src="{% editable_file "generalproject", hint: "Upload a visual (perfect size: 380px by 400px)", priority: 1 %}
              http://placehold.it/380x400
            {% endeditable_file %}">
          </div>
          <div class="span7">
            {% editable_long_text "General Projects description", hint: "General Projects description", priority: 1 %}
              Lorem ipsum dolor sit amet, consectetur adipisicing elit. Corporis, maxime magnam hic itaque cumque distinctio quas similique repellendus ullam voluptate. Velit, similique quae sunt cumque quam rem odit voluptatum assumenda.
            {% endeditable_long_text %}
          </div>
        </div>
        <ul>
          <li class="row">
            <ul>
              <li class="span4">
                <a href="#">
                  <img src="" alt="">
                  <p></p>
                </a>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
  </div>

{% endblock %}

{% block 'main' %}

  <div class="container page" id="main-why-choose">

    {% with_scope published: true %}

      {% for object in contents.projects %}

        {% assign icon = true %}
        {% assign link_url = false %}
        {% include 'page_content' %}

      {% endfor %}

    {% endwith_scope %}

  </div>

{% endblock %}